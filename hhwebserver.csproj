<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="credits\index.html" />
    <None Remove="favicon.ico" />
    <None Remove="index.html" />
    <None Remove="members\index.html" />
    <None Remove="not.txt" />
    <None Remove="not\index.html" />
  </ItemGroup>

  <ItemGroup>
    <ApplicationDefinition Include="favicon.ico" />
    <ApplicationDefinition Include="not\index.html" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="credits\index.html" />
    <EmbeddedResource Include="index.html" />
    <EmbeddedResource Include="members\index.html" />
    <EmbeddedResource Include="not.txt" />
  </ItemGroup>
<PropertyGroup>
  <OutputType>Exe</OutputType>
<RuntimeIdentifiers>win-x64</RuntimeIdentifiers>
<PublishTrimmed>true</PublishTrimmed>
<IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>

</PropertyGroup>

</Project>
